{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": ["last 2 versions", "safari >= 7"]
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "syntax-dynamic-import",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}